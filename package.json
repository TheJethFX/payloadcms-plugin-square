{
	"name": "payloadcms-plugin-square",
	"version": "1.2.1-next.1",
	"homepage": "https://github.com/TheJethFX/payloadcms-plugin-square",
	"description": "Unofficial Square plugin for Payload",
	"author": {
		"name": "Jeth"
	},
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/TheJethFX/payloadcms-plugin-square.git"
	},
	"maintainers": [
		{
			"name": "Jeth"
		}
	],
	"files": [
		"dist",
		"LICENSE.md",
		"README.md"
	],
	"keywords": [
		"payload",
		"payload-plugin",
		"cms",
		"plugin",
		"square",
		"ecommerce"
	],
	"bugs": {
		"url": "https://github.com/TheJethFX/payloadcms-plugin-square/issues"
	},
	"config": {
		"commitizen": {
			"path": "cz-conventional-changelog"
		}
	},
	"main": "./src/index.ts",
	"types": "./src/index.ts",
	"type": "module",
	"scripts": {
		"build": "tsc --project tsconfig.json",
		"build:all": "pnpm build && pnpm -C dev build",
		"build:app": "pnpm -C dev build",
		"commit": "git-cz",
		"clean": "rimraf {dist,*.tsbuildinfo,package}",
		"copyfiles": "copyfiles -u 1 \"src/**/*.{html,css,scss,ttf,woff,woff2,eot,svg,jpg,png}\" dist/",
		"dev": "pnpm -C dev dev",
		"lint": "eslint .",
		"lint:fix": "eslint . --fix",
		"prepack": "pnpm prepublishOnly",
		"prepublishOnly": "pnpm clean && pnpm build && pnpm copyfiles",
		"semantic-release": "semantic-release"
	},
	"peerDependencies": {
		"@payloadcms/db-mongodb": "^3.4.0",
		"@payloadcms/ui": "^3.4.0",
		"next": "^15.0.3",
		"payload": "^3.4.0",
		"react": "^19.0.0",
		"react-dom": "^19.0.0",
		"square": "^39.0.0"
	},
	"devDependencies": {
		"@payloadcms/eslint-config": "3.0.0",
		"@semantic-release/changelog": "^6.0.3",
		"@semantic-release/git": "^10.0.1",
		"@types/node": "22.7.5",
		"@types/react": "^19.0.0",
		"@types/react-dom": "^19.0.0",
		"cz-conventional-changelog": "^3.3.0",
		"copyfiles": "^2.4.1",
		"next": "15.0.3",
		"rimraf": "^6.0.1",
		"semantic-release": "^24.2.0",
		"square": "^39.0.0",
		"typescript": "5.5.3"
	},
	"publishConfig": {
		"access": "public",
		"registry": "https://registry.npmjs.org/",
		"main": "./dist/index.js",
		"types": "./dist/index.d.ts",
		"exports": {
			".": {
				"import": "./dist/index.js",
				"types": "./dist/index.d.ts",
				"default": "./dist/index.js"
			},
			"./client": {
				"import": "./dist/exports/client.js",
				"types": "./dist/exports/client.d.ts",
				"default": "./dist/exports/client.js"
			}
		}
	},
	"exports": {
		".": {
			"import": "./src/index.ts",
			"types": "./src/index.ts",
			"default": "./src/index.ts"
		},
		"./client": {
			"import": "./src/exports/client.ts",
			"types": "./src/exports/client.ts",
			"default": "./src/exports/client.ts"
		}
	}
}
