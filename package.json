{
	"name": "payloadcms-plugin-square",
	"version": "1.1.0-next.3",
	"homepage": "https://github.com/TheJethFX/payloadcms-plugin-square",
	"description": "Unofficial Square plugin for Payload",
	"author": "Jeth",
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/TheJethFX/payloadcms-plugin-square.git"
	},
	"maintainers": [
		{
			"name": "Jeth"
		}
	],
	"files": [
		"dist",
		"LICENSE.md",
		"README.md"
	],
	"keywords": [
		"payload",
		"cms",
		"plugin",
		"square",
		"ecommerce"
	],
	"bugs": {
		"url": "https://github.com/TheJethFX/payloadcms-plugin-square/issues"
	},
	"config": {
		"commitizen": {
			"path": "cz-conventional-changelog"
		}
	},
	"main": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"type": "module",
	"scripts": {
		"build": "tsc --project tsconfig.json",
		"build:all": "pnpm build && pnpm -C dev build",
		"build:app": "pnpm -C dev build",
		"commit": "git-cz",
		"clean": "rimraf {dist,*.tsbuildinfo,package}",
		"copyfiles": "copyfiles -u 1 \"src/**/*.{html,css,scss,ttf,woff,woff2,eot,svg,jpg,png}\" dist/",
		"dev": "pnpm -C dev dev",
		"lint": "eslint .",
		"lint:fix": "eslint . --fix",
		"prepack": "pnpm prepublishOnly",
		"prepublishOnly": "pnpm clean && pnpm build && pnpm copyfiles",
		"semantic-release": "semantic-release"
	},
	"peerDependencies": {
		"@payloadcms/db-mongodb": "^3.3.0",
		"@payloadcms/ui": "^3.3.0",
		"next": "^15.0.3",
		"payload": "^3.3.0",
		"react": "^19.0.0 || ^19.0.0-rc-66855b96-20241106",
		"react-dom": "^19.0.0 || ^19.0.0-rc-66855b96-20241106",
		"square": "^39.0.0"
	},
	"devDependencies": {
		"@payloadcms/eslint-config": "3.0.0",
		"@semantic-release/changelog": "^6.0.3",
		"@semantic-release/git": "^10.0.1",
		"@types/node": "22.7.5",
		"@types/react": "npm:types-react@19.0.0-rc.1",
		"@types/react-dom": "npm:types-react-dom@19.0.0-rc.1",
		"cz-conventional-changelog": "^3.3.0",
		"copyfiles": "^2.4.1",
		"next": "15.0.3",
		"rimraf": "^6.0.1",
		"semantic-release": "^24.2.0",
		"typescript": "5.5.3"
	},
	"pnpm": {
		"overrides": {
			"@types/react": "npm:types-react@19.0.0-rc.1",
			"@types/react-dom": "npm:types-react-dom@19.0.0-rc.1"
		}
	},
	"publishConfig": {
		"access": "public",
		"registry": "https://registry.npmjs.org/"
	},
	"exports": {
		".": {
			"import": "./dist/index.js",
			"types": "./dist/types/index.d.ts",
			"default": "./dist/index.js"
		}
	}
}
